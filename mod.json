{
	"geode": "4.1.2",
	"gd": {
		"win": "2.2074",
		"mac": "2.2074",
		"android": "2.2074"
	},
	"version": "v2.1.0",
	"id": "undefined0.editormusic",
	"name": "EditorMusic",
	"developer": "undefined0",
	"description": "Music in your editor, what's not to like?",
	"tags": [ "editor", "music", "enhancement" ],

	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": "1.19.0",
			"importance": "required"
		},
		{
			"id": "geode.custom-keybinds",
			"version": "1.10.1",
			"importance": "required",
			"platforms": [ "win" ]
		}
	],
	
	"links": {
		"source": "https://github.com/undefined06855/EditorMusic"
	},

	"resources": {
		"sprites": [ "./resources/*.png", "./resources/*/*.png" ]
	},

	"settings": {
		"volume": {
			"name": "Music volume",
			"type": "float",
			"default": 0.8,
			"min": 0,
			"max": 1,
			"control": {
				"slider": true,
				"input": true,
				"big-arrows": false,
				"arrows": false,
				"slider-step": 0.01
			}
		},
		"extra-songs-path": {
			"name": "Extra songs path",
			"type": "folder",
			"description": "If this is set to a path, EditorMusic will take songs from here as well as the config folder.",
			"default": "(none)"
		},
		"player-format": {
			"name": "Music player layout",
			"type": "string",
			"default": "default",
			"one-of": [ "default", "centered no album", "centered with album" ]
		},
		"round-corners": {
			"name": "Round corners of album cover",
			"type": "string",
			"default": "round",
			"one-of": [ "none", "round", "round with overlay" ]
		},
		"low-pass": {
			"name": "Enable low-pass filter",
			"description": "Enable or disable the low-pass filter that makes the sound \"muffled\" when you go deeper into menus.",
			"type": "bool",
			"default": true
		},
		"dev-title": {
			"type": "title",
			"name": "Other Options"
		},
		"menulayer-load": {
			"name": "Menulayer load",
			"description": "Makes song loading start when you enter the main menu, instead of when the mod loads.",
			"type": "bool",
			"default": false
		},
		"enable-log": {
			"name": "Enable logging",
			"description": "Enables logging for easier debugging - will make very large log sizes!!!",
			"type": "bool",
			"default": false
		},
		"reload-songs": {
			"name": "Reload songs",
			"type": "custom:reload-song-button"
		}
	}
}